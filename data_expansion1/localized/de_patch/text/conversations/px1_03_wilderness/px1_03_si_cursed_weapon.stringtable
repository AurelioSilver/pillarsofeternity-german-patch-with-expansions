    <Entry>
      <ID>6</ID>
      <DefaultText>[[Slot 3] befehlen, die Waffe zu ergreifen und herauszuziehen.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>[[Slot 4] befehlen, die Waffe zu ergreifen und herauszuziehen.]</DefaultText>
