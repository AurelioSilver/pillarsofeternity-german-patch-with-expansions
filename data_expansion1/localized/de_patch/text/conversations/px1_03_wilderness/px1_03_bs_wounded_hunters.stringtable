  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Die Wölfe haben unser Lager überlaufen. Diese Kreatur hat uns hierhergeschleppt."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Ich dachte, das wäre unser Ende."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Ich danke dir, Fremder. Wir finden allein zurück ins Dorf."</DefaultText>
      <FemaleText>"Ich danke dir, Fremde. Wir finden allein zurück ins Dorf."</FemaleText>
    </Entry>
