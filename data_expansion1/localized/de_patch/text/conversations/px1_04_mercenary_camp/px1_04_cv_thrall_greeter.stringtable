  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>Ein Mann wankt auf dich zu, als wäre er betrunken. Er bleibt einige Schritte vor dir stehen und blickt mit glasigen Augen auf einen Punkt über deiner Schulter.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>„Tanoss muss mit dir sprechen, [Player Race]. Komm, komm.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>„Komm, sprich mit Tanoss.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>„Achtung vor anderen Lehrlingen. Tiere, Eigenbrötler.“</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
