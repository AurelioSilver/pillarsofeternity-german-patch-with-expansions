<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\px1_01_ogre_camp\px1_01_bs_ogre</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Lauf, kleines Mäuschen."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Mögen Matronin Beregans Visionen uns leiten."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Die Winde schicken unserer Matronin in letzter Zeit böse Träume."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Wir gehen zur Batterie, wenn wir Kanonen brauchen. Aber nur, wenn es nicht anders geht."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Die Dorfbewohner lassen uns nun in Ruhe, ja?"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
